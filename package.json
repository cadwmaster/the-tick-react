{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^2.8.3",
    "@tensorflow/tfjs-converter": "^2.8.3",
    "@tensorflow/tfjs-core": "^2.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-lazy-load-image-component": "^1.5.1",
    "react-lottie": "^1.2.3",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage",
    "lint": "run-p lint:css lint:ts",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "stylelint --fix 'src/**/*.css'",
    "lint:fix": "run-p lint:*:fix",
    "lint:ts": "eslint --resolve-plugins-relative-to \"./node_modules/@ridedott/eslint-config\" \"**/*.tsx\"",
    "lint:ts:fix": "eslint --fix --resolve-plugins-relative-to \"./node_modules/@ridedott/eslint-config\" \"**/*.tsx\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ridedott/eslint-config": "^2.5.56",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lozad": "^1.9.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/react-lottie": "^1.2.5",
    "eslint": "^7.17.0",
    "husky": "^4.3.7",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": "^10.5.3",
    "msw": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
